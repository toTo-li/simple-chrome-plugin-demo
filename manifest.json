{
	"manifest_version": 2,
	"name": "Chrome插件demo",
	"version": "1.0.0",
	"description": "这是一个非常非常简单的Chrome插件示例，可以基于此进行业务开发",
	"author": "lijiayang",
	"icons":
	{
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": 
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": 
	[
		{
			"matches": ["https://www.baidu.com/*"],
			"js": ["content-script.js"]
		}
	],
	"web_accessible_resources": 
	[
		"inject.js"
	]
}